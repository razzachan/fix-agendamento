# Configurações do Vite
VITE_APP_NAME=Fix Fogões

# Configurações da API
VITE_API_URL=http://localhost:3000/api

# Push Notifications (Web Push VAPID)
VITE_VAPID_PUBLIC_KEY=

# Supabase Edge Functions (secrets configurados no Supabase)
# SUPABASE_URL e SUPABASE_SERVICE_ROLE_KEY são usados APENAS no backend (Edge Functions), não colocar no frontend
# No frontend, use SOMENTE VITE_SUPABASE_URL e VITE_SUPABASE_KEY (anon)

# Configurações do Supabase (Frontend usa ANON KEY)
VITE_SUPABASE_URL=https://hdyucwabemspehokoiks.supabase.co
VITE_SUPABASE_KEY=

# Configurações do Supabase (Backend/API usa SERVICE ROLE)
SUPABASE_URL=https://hdyucwabemspehokoiks.supabase.co
SUPABASE_SERVICE_ROLE_KEY=

# Calendário (Backend/API)
# Grade do calendário principal em minutos (ex.: 60 = 1h)
CALENDAR_SLOT_MIN=60
# Injetar horário de almoço como blackout
CALENDAR_INJECT_LUNCH=true
CALENDAR_LUNCH_START=12:00
CALENDAR_LUNCH_END=13:00

# Google Ads API Configuration
GOOGLE_ADS_CUSTOMER_ID=123-456-7890
GOOGLE_ADS_DEVELOPER_TOKEN=your_developer_token
GOOGLE_ADS_CLIENT_ID=your_client_id.apps.googleusercontent.com
GOOGLE_ADS_CLIENT_SECRET=your_client_secret
GOOGLE_ADS_REFRESH_TOKEN=your_refresh_token

