{
  "name": "webhook-ai",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx src/app.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/app.js",
    "test": "vitest run",
    "test:ci": "vitest run --reporter=verbose --no-coverage",
    "lint": "eslint --config eslint.config.mjs \"src/**/*.{ts,js}\" \"test/**/*.ts\" --max-warnings=0",
    "format": "prettier --write \"src/**/*.{ts,js}\" \"test/**/*.ts\"",
    "health:admin": "node ./scripts/admin-health-check.mjs",
    "health:admin:prod": "node ./scripts/admin-health-check-prod.mjs",
    "smoke:ai-router": "node ./scripts/ai-router-smoke.mjs",
    "smoke:e2e-gate": "node ./scripts/e2e-gate-orcamento.mjs",
    "smoke:orcamento": "node ./scripts/smoke-orcamento.mjs",
    "smoke:agendamento": "node ./scripts/smoke-agendamento.mjs",
    "smoke:all": "npm run smoke:ai-router && npm run smoke:e2e-gate && npm run smoke:orcamento && npm run smoke:agendamento",
    "smoke:all:local": "node ./scripts/smoke-all-local.mjs",
    "smoke:dev-local": "node ./scripts/smoke-dev-local.mjs"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.47.10",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "node-fetch": "^3.3.2",
    "qrcode": "^1.5.4",
    "socket.io": "^4.8.1",
    "vite_react_shadcn_ts": "file:..",
    "whatsapp-web.js": "^1.33.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.9",
    "@types/qrcode": "^1.5.5",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitest/coverage-v8": "^1.6.1",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "prettier": "^3.6.2",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.3",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.42.0",
    "vitest": "^1.6.0"
  },
  "vitest": {
    "setupFiles": [
      "./test/setup.ts"
    ]
  }
}
